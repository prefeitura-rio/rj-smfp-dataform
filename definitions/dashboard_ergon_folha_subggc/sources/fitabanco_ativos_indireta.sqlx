config {
    
    type: "table",
    name: "fitabanco_ativos_indireta",
    schema: "dashboard_ergon_folha_subggc",
    description: "Tabela dTb_Empresa_setor_inativos_pensionistas"

}

  select 
    distinct 
     safe_cast(mes_ano as DATE) data_folha
    ,safe_cast(numfunc||numvinc as STRING) id_funcao
    ,safe_cast(valorvan as FLOAT64) valor_bruto
    ,safe_cast(emp_codigo as INT64) id_empresa
    ,safe_cast(setor as INT64) id_setor
  from
   `rj-smfp.recursos_humanos_ergon_staging.fita_banco`
    where  mes_ano>='2021-01-01'
    and emp_codigo not in ('1','2','4')
    and emp_codigo < '80'
    and numdepen is null
    and numpens is null
    and setor != 'SEM LOTACAO'

  order by 
    data_folha desc 