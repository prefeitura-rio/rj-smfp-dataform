config {
    
    type: "table",
    name: "dbt_Empresa",
    schema: "dashboard_ergon_folha_subggc",
    description: "Esta tabela traz as informacoes de codigo e nome das empresas da administração direta e indireta"

}
  select 
    distinct 
     id_empresa
     ,nome_empresa
  from
    `rj-smfp.recursos_humanos_ergon.empresas`
  order by 
    id_empresa