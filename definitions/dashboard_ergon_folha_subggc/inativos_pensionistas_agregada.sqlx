config {
   type: "table",
   name: "inativos_pensionistas_agregada",
   schema: "dashboard_ergon_folha_subggc",
   description: "Folha adm publica agregada."

}

select 
* except(numdepen,numpens,setor)

FROM ${ref('toda_adm_publica_agregada')}
WHERE emp_codigo in ('2', '4' ) and numdepen is null and numpens is null and setor != 'SEM LOTACAO'
